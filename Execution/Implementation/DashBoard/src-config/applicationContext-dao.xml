<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true">

	<bean id="genericJdbcDao" class="com.gbm.dao.impl.GenericJdbcDaoImpl">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="groupDao" class="com.gbm.dao.impl.GroupDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	
	<bean id="performanceTodayDao" class="com.gbm.dao.impl.PerformanceTodayDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	
	<bean id="kpiTrendsDao" class="com.gbm.dao.impl.KPITrendsDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	
	<bean id="processMonitoringDao" class="com.gbm.dao.impl.ProcessMonitoringDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	
		<bean id="demographicTrendsDao" class="com.gbm.dao.impl.DemographicTrendsDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="pendingAnalysisDao" class="com.gbm.dao.impl.PendingAnalysisDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="genericDataFilterDao" class="com.gbm.dao.impl.GenericDataFilterDaoImpl">
			<property name="dataSource" ref="dataSource" />
	</bean>
	
	
	
	
	
</beans>